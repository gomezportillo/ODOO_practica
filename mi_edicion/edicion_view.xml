      <field name="arch" type="xml">
        <field name="sale_ok" position="after">
          <field name="is_course" select="0"/>
        </field>
        <field name="company_id" position="after">
          <field name="profesor" select="0"/>
        </field>
      </field>
    </record>
  </data>
</openerp>
